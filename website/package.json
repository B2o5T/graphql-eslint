{
  "name": "website",
  "version": "0.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "analyze": "ANALYZE=true yarn build",
    "build": "next build && next-sitemap --config next-sitemap.config.cjs && next export",
    "dev": "pnpm patch-browser && next",
    "patch-browser": "tsx ../scripts/patch-graphql-eslint-browser.ts",
    "prebuild": "tsx ../scripts/generate-docs.ts && pnpm patch-browser",
    "start": "next start",
    "theguild-nextra-algolia": "theguild-nextra-algolia"
  },
  "dependencies": {
    "@graphql-eslint/eslint-plugin": "workspace:*",
    "@monaco-editor/react": "4.5.1",
    "@radix-ui/react-icons": "1.3.0",
    "@radix-ui/react-select": "1.2.2",
    "@theguild/components": "5.2.1",
    "clsx": "2.0.0",
    "graphql": "16.7.1",
    "lodash.debounce": "4.0.8",
    "lodash.uniqwith": "4.5.0",
    "next": "13.4.13",
    "next-query-params": "4.2.3",
    "next-sitemap": "4.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "use-query-params": "2.2.1"
  },
  "devDependencies": {
    "@theguild/algolia": "1.1.9",
    "@theguild/tailwind-config": "0.3.0",
    "@types/lodash.debounce": "4.0.7",
    "@types/lodash.uniqwith": "4.5.7",
    "@types/node": "18.17.4",
    "@types/react": "18.2.19",
    "tailwindcss-radix": "2.8.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
