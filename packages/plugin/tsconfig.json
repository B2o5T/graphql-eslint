  "compilerOptions": {
    "target": "es2020",
    "module": "ESNext",
    "declaration": false,
    "noEmit": true,
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "allowJs": true,
    "moduleResolution": "node",
    "lib": ["ESNext"],
    "types": ["vitest/globals"],
    "resolveJsonModule": true
  }
}
